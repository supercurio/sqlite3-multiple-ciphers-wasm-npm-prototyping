{
  "name": "sqlite3-multiple-ciphers-wasm-checker",
  "version": "1.0.0",
  "description": "A command-line utility that fetches information about the latest release of the SQLite3MultipleCiphers project, focusing on WASM build files",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "sqlite3-multiple-ciphers-wasm-checker": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "debug": "node --inspect-brk src/index.js",
    "deno": "deno run src/index.js",
    "bun": "bun src/index.js"
  },
  "keywords": [
    "sqlite",
    "sqlite3",
    "wasm",
    "encryption",
    "multiple-ciphers",
    "github-release"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^3.3.0",
    "fs-extra": "^11.1.1",
    "decompress": "^4.2.1"
  },
  "engines": {
    "node": ">=14.16.0"
  }
}
